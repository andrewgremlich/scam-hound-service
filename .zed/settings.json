{
  "deno": { "enable": true },
  "format_on_save": "on",
  "formatter": {
    "external": {
      "command": "deno",
      "arguments": ["fmt", "-"]
    }
  },
  "languages": {
    "TypeScript": {
      "language_servers": [
        "deno",
        "!typescript-language-server",
        "!eslint",
        "..."
      ]
    },
    "TSX": {
      "language_servers": [
        "deno",
        "!typescript-language-server",
        "!eslint",
        "..."
      ]
    }
  }
}
